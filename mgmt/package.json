  "license": "MIT",
  "dependencies": {
    "axios": "^0.25.0",
    "cos-nodejs-sdk-v5": "^2.11.6",
    "dotenv": "^14.2.0",
    "ioredis": "^4.28.3",
    "jest": "^27.5.1",
    "js-core": "file:../js-core",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-mount": "^4.0.0",
    "koa-proxies": "^0.12.1",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa-static-cache": "^5.1.4",
    "koa2-cors": "^2.0.6",
    "moment": "^2.29.1",
    "semver": "^7.3.5",
