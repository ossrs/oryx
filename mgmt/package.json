{
  "name": "mgmt",
  "version":"1.0.169",
  "description": "API for srs-terraform mgmt",
  "main": "index.js",
  "scripts": {
    "start": "env NODE_ENV=development node .",
    "build": "cd ui && env PUBLIC_URL=/mgmt npm run build",
    "upgrade": "cd ui && env BUILD_PATH=./upgrade PUBLIC_URL=/mgmt npm run build",
    "postinstall": "cd ui && npm install",
    "test": "jest --watchAll=false --passWithNoTests",
    "testWatch": "jest --watchAll=true --passWithNoTests"
  },
  "jest": {
    "testPathIgnorePatterns": ["ui"]
  },
